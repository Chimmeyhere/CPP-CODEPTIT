//I'm HuyNho or you can call me Chimmey
#include <bits/stdc++.h>
using namespace std;
#define faster() ios_base::sync_with_stdio(false);cin.tie();cout.tie();
#define matrix vector <vector<long long>>
#define MOD 1000000007
const long long big = 1e6;
vector <long long> res;
int n, cnt;
matrix a, check;

int main() {
    faster();
    int t;
    cin >> t;
    while ( t-- )
    {
        int n, res = INT_MAX;
        cin >> n;
        vector <int> a(n);
        for (int i = 0 ; i < n ; i++)
        {
            cin >> a[i];
        }
        sort(a.begin(),a.end());
        for (int i = 1 ; i < n ; i++)
        {
            res = min(res,abs(a[i]-a[i-1]));
        }
        cout << res;
        if ( t != 0 )
        {
            cout << endl;
        }
    }
}

